{
    "manifest_version": 2,
    "name": "BackDate",
    "description": "See the original post date of any photo on the internet",
    "version": "1.0",
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*","*://*.cnn.com/*", "*://*.foxnews.com/*"],
        "js": ["script.js"],
        "css": ["styles.css"]
        
      }
    ]
  }
  
